  "publishConfig": {
    "cache": "cache/.npm",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "browserslist": [
    "last 3 versions",
    "not ie <= 11",
    "not dead",
    "> 5%"
